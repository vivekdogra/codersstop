#nav-top {
	position:relative;
	font-size:12px;
	font-weight:550;
	width:320px;
	margin: 10px 0 0 920px;
	padding-top:5px;
	border-bottom-style:solid;
	border-bottom-width:2px;
	background: rgb(228,245,252); /* Old browsers */
	background: -moz-linear-gradient(top, rgba(228,245,252,1) 0%, rgba(159,216,239,1) 69%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(228,245,252,1)), color-stop(69%,rgba(159,216,239,1))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, rgba(228,245,252,1) 0%,rgba(159,216,239,1) 69%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, rgba(228,245,252,1) 0%,rgba(159,216,239,1) 69%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top, rgba(228,245,252,1) 0%,rgba(159,216,239,1) 69%); /* IE10+ */
	background: linear-gradient(to bottom, rgba(228,245,252,1) 0%,rgba(159,216,239,1) 69%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#e4f5fc', endColorstr='#9fd8ef',GradientType=0 ); /* IE6-9 */
}
	
#nav-top ul {
	margin-left:10px;
}
  
#nav-top li {
	padding-right:15px;
	display:inline;
}

#nav-top a {
	color:black;
}
